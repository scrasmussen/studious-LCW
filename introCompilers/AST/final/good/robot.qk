/*
 *  Demonstrates inheritance and some math statements
 * 
 */


class Robot() {
    this.name = "Bozotron";
    this.age = 42;
    this.strength = 100;

    def destroy_city() {
       " printing from method \n ".PRINT();
    }
}


class SmartRobot(iq: Int) extends Robot {
    this.name = "Einsteinitron";  // OK, compatible
    this.age = 48;
    this.strength = 101;
    this.iq = 4 * this.age;
}

rob=Robot();
rob.destroy_city();
