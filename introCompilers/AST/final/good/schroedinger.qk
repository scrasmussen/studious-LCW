/*
*  File demonstrates that the if, elif, and else statements work
*/

class Schroedinger(box: Int) {
    box.PRINT();
    "\n".PRINT();
    if (box < -1) {
       this.living = 1;
       this.living.PRINT();
       "\n".PRINT();
    }
    elif (box < 6) {
      this.is = 71;
    }   
    else {
       this.dead = 0;
    }
}

 x=-5;
 sc1=Schroedinger(x);
